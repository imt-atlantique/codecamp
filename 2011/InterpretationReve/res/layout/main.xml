<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
    android:id="@+id/widget28"
	android:layout_width="fill_parent" 
	android:layout_height="fill_parent"
	android:orientation="vertical" 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:text="@string/app_name" 
	android:background="@drawable/background">
	
    <TextView 
    android:id="@+id/txt_marque" 
    android:layout_width="wrap_content" 
    android:layout_height="wrap_content"
    android:text="L'interprétation des rêves, fait à TELECOM BRETAGNE, le 20/01/2011."
    android:textSize="4pt" 
    android:textColor="#000000"
    android:layout_marginTop="410dip" 
    android:layout_marginLeft="20dip"
    />

    <TextView 
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="-420dip"
    android:layout_marginLeft="11dip"
    android:text="Taper un sujet de reve"
    android:textColor="#000066"  
    android:textSize="6pt"/>
    <AutoCompleteTextView 
    android:id="@+id/txt_name" 
    android:layout_marginLeft="5dp" 
    android:layout_width="200dip" 
    android:layout_height="60dip" 
    android:layout_marginTop="5dip">
    </AutoCompleteTextView>
	<ImageButton 
	android:id="@+id/btn_ok" 
	android:layout_height="30dip" 
	android:layout_width="100dip" 
	android:textSize="6.5pt"
	android:layout_marginTop="-50dip" 
	android:layout_marginLeft="210dip" 
	android:background="@drawable/selector">
	</ImageButton>
</LinearLayout>